{
   "$schema": "https://json-schema.org/draft/2019-09/schema",
   "$id": "http://example.com/example.json",
   "type": "object",
   "required": [
      "empNumber",
      "email",
      "isAdmin"
   ],
   "properties": {
      "empNumber": {
         "type": "integer"
      },
      "email": {
         "type": "string",
         "format": "email",
         "minLength": "5",
         "maxLength": "30"
      },
      "isAdmin": {
         "type": "boolean",
         "default": false
      }
   }
}